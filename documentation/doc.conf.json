{
  "plugins": [
    "node_modules/jsdoc-vuejs",
    "jsdoc-vuex-plugin",
    "plugins/markdown"
  ],
  "source": {
    "include": ["src/"],
    "exclude": ["src/documentation/documentation_postprocess.js"],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "\\.(spec.js)$"
  },
  "recurseDepth": 10,
  "opts": {
    "encoding": "utf8",
    "destination": "./documentation/html/",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "FAIRsharing.org",
    "copyright": "DocStrap Copyright Â© 2012-2015 The contributors to the JSDoc3 and DocStrap projects.",
    "theme": "yeti",
    "linenums": true,
    "inverseNav": true,
    "dateFormat": "ddd MMM Do YYYY"
  },
  "tags": {
    "allowUnknownTags": true
  }
}
